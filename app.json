{
  "expo": {
    "name": "ESP32 Weather Station",
    "description": "A React Native application for viewing data from the ESP32 weather station project!",
    "slug": "esp32-weather-station-client",
    "privacy": "hidden",
    "version": "1.0.0",
    "platforms": ["android"],
    "githubUrl": "https://github.com/dancs-dev/weather-station-client-app",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#696969"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "dev.dancs.esp32_weather_station_client",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#696969"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
